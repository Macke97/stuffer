{
    "postprocess": {
        "rules": [
            {
                "match": "(.*)",
                "apply":"gzip",
                "options":{}
            }
        ]
    },
    "modifiers": {
        "resize": {
            "small": [256, "auto", 80],
            "medium": [512, "auto", 80],
            "big": [1024, "auto", 80],
            "thumb": [150, 150, 80, "cover"]
        }
    },

    "spaces": {
        "public": {
            "modifiers": {
                "resize": {
                    "medium": [ 250, "auto", 100 ]
                }
            },
            "postprocess": {}
        },
        "_user-22": {
            "modifiers": {
                "resize": {
                    "small": [ 80, "auto", 20 ]
                }
            }
        }
    }
}
